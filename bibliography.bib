@INPROCEEDINGS { saurel16,
  TITLE = { {A Simulation Framework for Simultaneous Design and Control of Passivity Based Walkers} },
  AUTHOR = { Saurel, Guilhem and Carpentier, Justin and Mansard, Nicolas and Laumond, Jean-Paul },
